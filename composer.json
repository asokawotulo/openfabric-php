{
	"name": "asokawotulo/openfabric-php",
	"description": "OpenFabric PHP SDK",
	"version": "1.0.0",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Asokawotulo",
			"email": "asokawotulo2@gmail.com"
		}
	],
	"minimum-stability": "stable",
	"require": {
		"php": ">=8.0.0",
		"guzzlehttp/guzzle": "^7.8"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.5",
		"vlucas/phpdotenv": "^5.6",
		"symfony/var-dumper": "^6.0"
	},
	"autoload": {
		"psr-4": {
			"OpenFabric\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"OpenFabric\\": [
				"tests/",
				"tests/OpenFabric/"
			]
		}
	}
}
